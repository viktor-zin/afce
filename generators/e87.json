{
	"name": {
		"ru_RU": "\u0410\u043B\u0433\u043E\u0440\u0438\u0442\u043C\u0438\u0447\u0435\u0441\u043A\u0438\u0439 \u044F\u0437\u044B\u043A \u0415\u0440\u0448\u043E\u0432\u0430",
		"en_EN": "Ershov's algorithmic language",
		"uk_UA": "\u0410\u043b\u0433\u043e\u0440\u0438\u0442\u043c\u0456\u0447\u043d\u0430 \u043c\u043e\u0432\u0430 \u0404\u0440\u0448\u043e\u0432\u0430"
	},

	"algorithm" : {
		"template" : "\u0430\u043B\u0433 \u0410\u043B\u0433\u043E\u0440\u0438\u0442\u043C\n\u043D\u0430\u0447%branch1%\n\u043A\u043E\u043D"
		},
	"if": {
		"template":"\u0435\u0441\u043B\u0438 %cond%\n\u0442\u043E%branch1%\n\u0438\u043D\u0430\u0447\u0435%branch2%\n\u0432\u0441\u0435"
		},
	"for": {
		"template":"\u043D\u0446 \u0434\u043B\u044F %var% \u043E\u0442 %from% \u0434\u043E %to%%branch1%\n\u043A\u0446"
	},
		"pre": {
			"template":"\u043D\u0446 \u043F\u043E\u043A\u0430 %cond%%branch1%\n\u043A\u0446"
		},
		"post": {
			"template":"%branch1%\n\u043D\u0446 \u043F\u043E\u043A\u0430 %cond%%branch1%\n\u043A\u0446"
		},
		"assign": {
			"template":"%dest% := %src%"
		},
		"process": {
			"template": "%text%"

		},
		"io": {
			"template": "\u0432\u0432\u043E\u0434 %vars%",
			"list": ["vars"],
			"separator": ",",
			"glue": ", "
		},
		"ou": {
			"template": "\u0432\u044B\u0432\u043E\u0434 %vars%",
			"list": ["vars"],
			"separator": ",",
			"glue": ", "
			}
		}