{
	"algorithm" : {
		"template" : "def main():%branch1%\n"
	},
	"if": {
		"template":"if %cond%:%branch1%\nelse: %branch2%\n"
	},
	"for": {
		"template":"for %var% in xrange(%from%, %to%):%branch1%\n"
	},
	"pre": {
		"template":"while %cond%:%branch1%\n"
	},
	"post": {
		"template":"while True:%branch1%\n\tif %cond%:\n\t\tbreak"
	},
	"assign": {
		"template":"%left% = %right%;"
	},
	"process": {
		"template": "%text%;"
	},
	"io": {
		"template": "%vars%",
		"list": ["vars"],
		"separator": ",",
		"glue": "\n",
		"suffix": " = input()"
	},

	"ou": {
		"template": "print(%vars%)",
		"list": ["vars"],
		"separator": ",",
		"glue": ", "

	}
}